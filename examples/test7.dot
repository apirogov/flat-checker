/* e.g. try this formulae:
   (Fd & Fa)
   (Fd & Fb)
 */
digraph G {
  0 [props="x"];
  1 [props="a"];
  2 [props="b"];
  3 [props="c"];
  4 [props="d"];
  0 -> 1;
  0 -> 2;
  1 -> 1 [updates="v += 1"];
  2 -> 2 [updates="w += 1"];
  1 -> 3 [updates="u += 1"];
  2 -> 3 [updates="u += 1"];
  3 -> 4 [guards="[u>0,v>=10],[u>0,w>=10]"];
  4 -> 4;
}
