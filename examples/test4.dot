/* e.g. try this formulae:
  Fz
 */
digraph G {
  0 [props="{x}"];
  1 [props="{a}"];
  2 [props="{b}"];
  3 [props="{c}"];
  4 [props="{d}"];
  5 [props="{w}"];
  6 [props="{y}"];
  7 [props="{z}"];
  0 -> 1;
  1 -> 2 [updates="{x += 2}"];
  2 -> 2 [updates="{x += 2}"];
  2 -> 1;
  1 -> 3 [updates="{y-=3}"];
  3 -> 3 [updates="{y += -3}"];
  3 -> 1;
  1 -> 4 [guards="{[x >= 10, 3x + 2y = 0]}"];
  4 -> 4;
  0 -> 5;
  5 -> 5 [updates="{x += 5, y-= 7}"];
  5 -> 6 [guards="{[x = 100]}"];
  6 -> 6 [updates="{y += 7, x -=5}"];
  6 -> 7 [guards="{[x - y = 0]}"];
  7 -> 7;
}
